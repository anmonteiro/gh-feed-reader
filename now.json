{
  "name": "gh-feed",
  "alias": "gh-feed",
  "version": 2,
  "builds": [
    {
      "src": "bootstrap",
      "use": "now-custom-runtime"
    },
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api",
      "dest": "/bootstrap"
    }
  ]
}
